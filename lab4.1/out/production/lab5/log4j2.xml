<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="WARN">
    <Appenders>
        <Console target="SYSTEM_OUT" name="Console">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.trees" additivity="false" level="debug">
            <AppenderRef level="debug" ref="Console"/>
        </Logger>
        <Root additivity="false" level="debug">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>

</Configuration>